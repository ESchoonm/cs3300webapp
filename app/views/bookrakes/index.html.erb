<p id="notice"><%= notice %></p>

<h1>Bookrakes</h1>

<table>
  <thead>
    <tr>
      <th>Db</th>
      <th>Book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookrakes.each do |bookrake| %>
      <tr>
        <td><%= bookrake.db %></td>
        <td><%= bookrake.Book %></td>
        <td><%= link_to 'Show', bookrake %></td>
        <td><%= link_to 'Edit', edit_bookrake_path(bookrake) %></td>
        <td><%= link_to 'Destroy', bookrake, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bookrake', new_bookrake_path %>
